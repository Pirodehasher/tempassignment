									"const expectedPrice  = parseInt(pm.iterationData.get(\"price\"), 10);\r",
									"const expectedStock  = parseInt(pm.iterationData.get(\"stock\"), 10);\r",
									"\r",
									"// Find matching book in response\r",
									"const matchingBook = res.data.find(b => \r",
									"    (b.title || \"\").trim()  === expectedTitle &&\r",
									"    (b.author || \"\").trim() === expectedAuthor\r",
									");\r",
									"\r",
									"pm.test(`Book \"${expectedTitle}\" should exist in store`, function () {\r",
									"    pm.expect(matchingBook, `Book not found: ${expectedTitle}`).to.exist;\r",
									"});\r",
									"\r",
									"if (matchingBook) {\r",
									"    pm.test(`Price matches for \"${expectedTitle}\"`, function () {\r",
									"        pm.expect(matchingBook.price, \"Price mismatch\").to.eql(expectedPrice);\r",
									"    });\r",
									"\r",
									"    pm.test(`Stock matches for \"${expectedTitle}\"`, function () {\r",
									"        pm.expect(matchingBook.stock, \"Stock mismatch\").to.eql(expectedStock);\r",
									"    });\r",
